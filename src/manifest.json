{
  "name": "procrastin8",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "*://*/*"
  ],
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["index.js"]
  }],
  "background": {
    "scripts": [ "background.js" ]
  },
  "options_page": "options.html"
}
